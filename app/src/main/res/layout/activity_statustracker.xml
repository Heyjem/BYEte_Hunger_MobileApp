<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".statustracker">

    <EditText
        android:id="@+id/et1_statustracker"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_marginTop="80dp"
        android:ems="10"
        android:text="TRACK YOUR DONATION"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:textSize="18sp"
        android:fontFamily="@font/montserrat_semibold"
        android:backgroundTint="#2eb674"
        android:background="@drawable/edittext_rounded_corners"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et1_statustracker"
        app:srcCompat="@drawable/tracker_icon" />

    <TextView
        android:id="@+id/tv_statustracker_uidtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/montserrat_semibold"
        android:text="UID: "
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView26" />

    <TextView
        android:id="@+id/tv_statustracker_uidvalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:clickable="true"
        android:fontFamily="@font/montserrat_semibold"
        android:text="123456789976"
        android:textColor="#2eb674"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/tv_statustracker_uidtitle"
        app:layout_constraintTop_toBottomOf="@+id/imageView26" />

    <TextView
        android:id="@+id/tv_statustracker_donationconfirmed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="124dp"
        android:fontFamily="@font/montserrat_regular"
        android:textStyle="bold"
        android:text="Donation Confirmed"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_uidvalue" />

    <TextView
        android:id="@+id/tv_statustracker_donationconfirmeddate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="00/00/00"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationconfirmed" />

    <TextView
        android:id="@+id/tv_statustracker_donationconfirmedtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="--:--:--"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationconfirmeddate" />

    <TextView
        android:id="@+id/tv_statustracker_donationpicked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="60dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="Donation has been picked up"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationconfirmedtime" />

    <TextView
        android:id="@+id/tv_statustracker_donationpickeddate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="00/00/00"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationpicked" />

    <TextView
        android:id="@+id/tv_statustracker_donationpickedtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="--:--:--"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationpickeddate" />


    <TextView
        android:id="@+id/tv_statustracker_donationdropped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="46dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="Donation has been dropped off"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationpickedtime" />

    <TextView
        android:id="@+id/tv_statustracker_donationdroppeddate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="00/00/00"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationdropped" />

    <TextView
        android:id="@+id/tv_statustracker_donationdroppedtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="--:--:--"
        android:textStyle="bold"
        android:textColor="#646464"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationdroppeddate" />

    <TextView
        android:id="@+id/tv_statustracker_donationdonated"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="Donation has been donated\nThank You!"
        android:textStyle="bold"
        android:textColor="#ffc91e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationdroppedtime" />
    <TextView
        android:id="@+id/tv_statustracker_donationdonateddate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="00/00/00"
        android:textStyle="bold"
        android:textColor="#ffc91e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationdonated" />

    <TextView
        android:id="@+id/tv_statustracker_donationdonatedtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/montserrat_regular"
        android:text="--:--:--"
        android:textStyle="bold"
        android:textColor="#ffc91e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statustracker_donationdonateddate" />

</androidx.constraintlayout.widget.ConstraintLayout>